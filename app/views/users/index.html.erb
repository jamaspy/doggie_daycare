


<table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <th scope="row"><%=user.id%></th>
      <td><%= link_to user.name, user_path(user.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Add New User", new_user_path %>