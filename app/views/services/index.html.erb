



<table class="table mt-5">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  
  <tbody>
  <% @services.each do |service| %>
    <tr>
      <th scope="row"><%= link_to service.name, service_path(service.id) %></th>
      <td>$<%= service.price %></td>
    </tr>
    <% end %>
  </tbody>
</table>

