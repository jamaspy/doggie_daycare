
<div class="form-group mt-3">
<%= form_for @dog do |f| %>
    <%= f.label :name, :class => "mt-2" %>
    <%= f.text_field :name, :autofocus => true, :class => "form-control"  %>
    <%= f.label :breed, :class => "mt-2"%>
    <%= f.text_field :breed, :class => "form-control" %>
   <%= f.label :color, :class => "mt-2"%>
    <%= f.text_field :color, :class => "form-control" %>
    <%= f.label :sex, :class => "mt-2"%>
    <%= f.text_field :sex, :class => "form-control" %>
    <%= f.label :Vaccination, :class => "mt-2"%>
    <%= f.text_field :vac_1, :class => "form-control" %>
    <%= f.label :Vaccination, :class => "mt-2"%>
    <%= f.text_field :vac_2, :class => "form-control" %>
    <%= f.label :flea, :class => "mt-2"%>
    <%= f.date_field :flea, :class => "form-control" %>
    <%= f.label :worm, :class => "mt-2"%>
    <%= f.date_field :flea, :class => "form-control" %>
    <%= f.label :image, :class => "mt-2"%>
    <%= f.url_field :image, :class => "form-control" %>

    <%= f.submit "Create New Dog", :class => "btn btn-primary mt-3" %>
<% end %>