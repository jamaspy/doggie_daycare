<%= form_for @dog do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, :autofocus => true %>
    <%= f.label :breed%>
    <%= f.text_field :breed%>
   <%= f.label :color%>
    <%= f.text_field :color%>
    <%= f.label :sex%>
    <%= f.text_field :sex%>
    <%= f.label :Vaccination%>
    <%= f.text_field :vac_1%>
    <%= f.label :Vaccination%>
    <%= f.text_field :vac_2%>
    <%= f.label :flea%>
    <%= f.date_field :flea%>
    <%= f.label :worm%>
    <%= f.date_field :flea%>
    <%= f.label :image%>
    <%= f.url_field :image%>

    <%= f.submit %>
<% end %>