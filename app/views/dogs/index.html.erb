

<% @dogs.each do |dog| %>

<div class="col-lg-4 mt-5">
<%= image_tag dog.image, :class => "thumbnail mb-3"%>

<p><span class = "bold-text">Name:</span> <span class = "index-to-show-link"><%= link_to dog.name, dog_path(dog.id) %></span> </p>
<p><span class = "bold-text">Breed:</span> <%= dog.breed %></p>
<p><span class = "bold-text">Color:</span> <%= dog.color %></p>
<p><span class = "bold-text">Sex:</span> <%= dog.sex%></p>


</div>
<% end %>