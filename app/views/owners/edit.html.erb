<%= form_for @owner do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, :autofocus => true %>
    <%= f.label :email%>
    <%= f.email_field :email%>
   <%= f.label :mobile%>
    <%= f.text_field :mobile%>
    <%= f.label :address%>
    <%= f.text_field :address%>

    <%= f.submit %>
<% end %>