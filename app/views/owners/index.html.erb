<h1>All Owners</h1>

<% @owners.each do |owner| %>
<p><span class = "bold-text">Name:</span> <span class = "index-to-show-link"><%= link_to owner.name, owner_path(owner.id) %></span> </p>
<p><span class = "bold-text">Email:</span> <%= owner.email %></p>
<p><span class = "bold-text">Mobile:</span> <%= owner.mobile %></p>
<p><span class = "bold-text">Address:</span> <%= owner.address%></p>
<% end %>